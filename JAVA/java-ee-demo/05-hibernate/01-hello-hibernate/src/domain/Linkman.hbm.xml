<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="domain.Linkman" table="cst_linkman"  >

		<id name="lkm_id" column="lkm_id">
			<generator class="native"></generator>
		</id>

		<version name="version"></version>

		<property name="lkm_name" column="lkm_name"></property>
		<property name="lkm_gender" column="lkm_gender"></property>
		<property name="lkm_phone" column="lkm_phone"></property>
		<property name="lkm_mobile" column="lkm_mobile"></property>
		<property name="lkm_email" column="lkm_email"></property>
		<property name="lkm_qq" column="lkm_qq"></property>
		<property name="lkm_position" column="lkm_position"></property>
		<property name="lkm_memo" column="lkm_memo"></property>

		<!-- 先配置多方 name 当前JavaBean中的属性 class 属性的全路径 column 外键的字段 -->
		<!-- cascade="save-update,delete" -->
		<many-to-one name="customer" class="domain.Customer"	column="lkm_cust_id"  cascade="save-update,delete"  fetch="join" lazy="proxy" ></many-to-one>

	</class>
</hibernate-mapping>